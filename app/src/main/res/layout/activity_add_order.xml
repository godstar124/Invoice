                            android:textColorHint="@android:color/darker_gray"
                            android:layout_toLeftOf="@+id/btn_new_customer"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_marginRight="10dp"
