                            android:inputType="textCapSentences"
                            android:textColor="@android:color/black"
                            android:textSize="20dp" />

                        <EditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:id="@+id/et_billing_address1"
